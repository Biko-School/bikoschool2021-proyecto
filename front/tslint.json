{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "member-access": [
      true,
      "no-public"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-empty-interface": [
      false
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-floating-promises": false,
    "max-classes-per-file": false,
    "jsx-no-lambda": false
  }
}
